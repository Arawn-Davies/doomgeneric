{
    "configurations": [
        {
            "name": "EE (Open Source SDK)",
            "includePath": [
                "${workspaceFolder}/**",
                "${PS2SDK}/common/include/**",
                "${PS2SDK}/common/include/sys/**",
                "${PS2SDK}/ee/include/**",
                "${PS2SDK}/ports/include/**",
                "${PS2SDK}/ports/include/SDL/**",
                "${PS2SDK}/ports/include/libmodplug/**",
                "${PS2SDK}/ports/include/vorbis/**",
                "${PS2DEV}/ee/include/**",
                "${PS2SDK}/ee/include/sys/**",
                "${PS2DEV}/iop/lib/gcc/mipsel-none-elf/15.2.0/include**",

                "${GSKIT}/include/**"
            ],
            "defines": [
                "_EE"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        },
        {
            "name": "IOP (Open Source SDK)",
            "includePath": [
                "${workspaceFolder}/**",
                "${PS2SDK}/common/include/**",
                "${PS2SDK}/iop/include/**",
                "${PS2DEV}/iop/include/**",
                "${PS2DEV}/iop/lib/gcc/mipsel-none-elf/15.2.0/include**",
                "${PS2DEV}/iop/iop/sys-include/**"
            ],
            "defines": [
                "_IOP"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}